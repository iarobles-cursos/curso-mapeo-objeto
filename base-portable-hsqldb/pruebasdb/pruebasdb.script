CREATE MEMORY TABLE PUBLIC.DATOS_USUARIOS(ID INTEGER GENERATED BY DEFAULT AS SEQUENCE PUBLIC.DATOS_USUARIOS_ID_SEQ NOT NULL PRIMARY KEY,BIOGRAFIA CLOB(1G),FECHA_REGISTRO DATE)
CREATE MEMORY TABLE PUBLIC.DATOS_ARTICULOS(ID INTEGER GENERATED BY DEFAULT AS SEQUENCE PUBLIC.DATOS_ARTICULOS_ID_SEQ NOT NULL PRIMARY KEY,OBSERVACIONES CLOB(1G))
CREATE MEMORY TABLE PUBLIC.USUARIOS(ID INTEGER GENERATED BY DEFAULT AS SEQUENCE PUBLIC.USUARIOS_ID_SEQ NOT NULL PRIMARY KEY,DATOS_AUTOR_ID INTEGER,NOMBRE VARCHAR(600),EMAIL VARCHAR(500),PASSWORD VARCHAR(500),ROL VARCHAR(500),ACTIVO BOOLEAN,FOREIGN KEY(DATOS_AUTOR_ID) REFERENCES PUBLIC.DATOS_USUARIOS(ID))
CREATE MEMORY TABLE PUBLIC.ARTICULOS(ID INTEGER GENERATED BY DEFAULT AS SEQUENCE PUBLIC.ARTICULO_ID_SEQ NOT NULL PRIMARY KEY,USUARIO_ID INTEGER,DATOS_ARTICULO_ID INTEGER,TITULO VARCHAR(500),CONTENIDO CLOB(1G),FECHA_CREACION DATE,FOREIGN KEY(USUARIO_ID) REFERENCES PUBLIC.USUARIOS(ID),FOREIGN KEY(DATOS_ARTICULO_ID) REFERENCES PUBLIC.DATOS_ARTICULOS(ID))
CREATE MEMORY TABLE PUBLIC.COMENTARIOS(ID INTEGER GENERATED BY DEFAULT AS SEQUENCE PUBLIC.COMENTARIO_ID_SEQ NOT NULL PRIMARY KEY,ARTICULO_ID INTEGER,COMENTARIO CLOB(1G),COMENTARISTA VARCHAR(500),FECHA_CREACION DATE,FOREIGN KEY(ARTICULO_ID) REFERENCES PUBLIC.ARTICULOS(ID))
CREATE MEMORY TABLE PUBLIC.CATEGORIAS(ID INTEGER GENERATED BY DEFAULT AS SEQUENCE PUBLIC.CATEGORIA_ID_SEQ NOT NULL PRIMARY KEY,NOMBRE VARCHAR(200))
CREATE MEMORY TABLE PUBLIC.CATEGORIAS_ARTICULOS(CATEGORIA_ID INTEGER,ARTICULO_ID INTEGER,PRIMARY KEY(CATEGORIA_ID,ARTICULO_ID),FOREIGN KEY(CATEGORIA_ID) REFERENCES PUBLIC.CATEGORIAS(ID),FOREIGN KEY(ARTICULO_ID) REFERENCES PUBLIC.ARTICULOS(ID))
CREATE MEMORY TABLE PUBLIC.DEPARTAMENTOS(ID INTEGER GENERATED BY DEFAULT AS SEQUENCE PUBLIC.DEPARTAMENTO_ID_SEQ NOT NULL PRIMARY KEY,NOMBRE VARCHAR(200))
CREATE MEMORY TABLE PUBLIC.DEPARTAMENTOS_USUARIOS(DEPARTAMENTO_ID INTEGER,USUARIO_ID INTEGER,PRIMARY KEY(DEPARTAMENTO_ID,USUARIO_ID),FOREIGN KEY(DEPARTAMENTO_ID) REFERENCES PUBLIC.DEPARTAMENTOS(ID),FOREIGN KEY(USUARIO_ID) REFERENCES PUBLIC.USUARIOS(ID))
CREATE MEMORY TABLE PUBLIC.DOMICILIOS(ID INTEGER GENERATED BY DEFAULT AS SEQUENCE PUBLIC.DOMICILIOS_ID_SEQ NOT NULL PRIMARY KEY,USUARIO_ID INTEGER,CALLE VARCHAR(300),NUMERO VARCHAR(100),COLONIA VARCHAR(300),CIUDAD VARCHAR(300),FOREIGN KEY(USUARIO_ID) REFERENCES PUBLIC.USUARIOS(ID))
CREATE MEMORY TABLE PUBLIC.RATINGS(ID INTEGER GENERATED BY DEFAULT AS SEQUENCE PUBLIC.RATINGS_ID_SEQ NOT NULL PRIMARY KEY,RATING INTEGER,ARTICULO_ID INTEGER,FOREIGN KEY(ARTICULO_ID) REFERENCES PUBLIC.ARTICULOS(ID))
CREATE MEMORY TABLE PUBLIC.TELEFONOS(ID INTEGER GENERATED BY DEFAULT AS SEQUENCE PUBLIC.TELEFONOS_ID_SEQ NOT NULL PRIMARY KEY,USUARIO_ID INTEGER,TELEFONO VARCHAR(300),FOREIGN KEY(USUARIO_ID) REFERENCES PUBLIC.USUARIOS(ID))
CREATE MEMORY TABLE PUBLIC.EMAILS_ADICIONALES(ID INTEGER GENERATED BY DEFAULT AS SEQUENCE PUBLIC.EMAILS_ADICIONALES_ID_SEQ NOT NULL PRIMARY KEY,USUARIO_ID INTEGER,EMAIL VARCHAR(300),FOREIGN KEY(USUARIO_ID) REFERENCES PUBLIC.USUARIOS(ID))
CREATE MEMORY TABLE PUBLIC.VENTAS(ORDEN INTEGER,CODIGO_BARRAS VARCHAR(300),NOMBRE_PRODUCTO VARCHAR(300),CANTIDAD INTEGER,PRIMARY KEY(ORDEN,CODIGO_BARRAS))
CREATE MEMORY TABLE PUBLIC.INSCRIPCIONES(MATRICULA VARCHAR(300),CURSO VARCHAR(300),FECHA_INSCRIPCION DATE,PRIMARY KEY(MATRICULA,CURSO))
